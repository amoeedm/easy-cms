 {
    "manifest_version": 2,
    
	"name": "Easy CMS Populate",
	"description": "Quickly populate a Google Sheet",
	"version": "1.0",
	"author": "Abdul Moeed Mohammad",
	
	"background": {
        "scripts": ["background.js"],
        "persistent": true
    },
	
    "browser_action": {
        "default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Easy CMS Populate"
    },
	
    "permissions" : [
		"tabs",
		"identity",
		"https://*.googleapis.com/*"
	],
	
	"oauth2": {
        "client_id": "<Insert Client ID>",
        "scopes": ["https://www.googleapis.com/auth/spreadsheets"]
    },
	
	"key": "<Insert encrypted key>"
 }